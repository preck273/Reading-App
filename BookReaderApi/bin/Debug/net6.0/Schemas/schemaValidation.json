{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "definitions": {
    "Welcome8Element": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "account": {
          "type": "string"
        },
        "closingAvailableBalance": {
          "type": "string"
        },
        "closingBalance": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "customDescription": {
          "type": "string",
          "maxLength": 50
        },
        "forwardAvailableBalance": {
          "type": "string"
        },
        "openingBalance": {
          "type": "string"
        },
        "relatedMessage": {
          "type": "string"
        },
        "sequenceNumber": {
          "type": "string"
        },
        "statementNumber": {
          "type": "string"
        },
        "transactionReference": {
          "type": "string"
        },
        "transactionDate": {
          "type": "string"
        },
        "category": {
          //"type": "string",
          "enum": [ "Beverage", "Equipment", "Food", "Internal_Cost", "Maintenance", "Rent", "Staff", "Customer", "none" ]
        },
        "debitCredit": {
          "type": "string",
          "enum": [ "Debit", "Credit" ]
        }
      },
      "required": [
        "account",
        "closingAvailableBalance",
        "closingBalance",
        "description",
        "customDescription",
        "id",
        "openingBalance",
        "relatedMessage",
        "sequenceNumber",
        "statementNumber",
        "transactionReference",
        "transactionDate",
        "category",
        "debitCredit"
      ],
      "title": "Welcome8Element"
    }
  }
}
